APP_NAME=FloatChat API
ENV=dev
# SQLite (dev): sqlite+aiosqlite:///./floatchat.db
# Postgres example (async): postgresql+asyncpg://USER:PASSWORD@HOST:5432/DBNAME
DATABASE_URL=sqlite+aiosqlite:///./floatchat.db
API_PREFIX=/api
# Comma-separated CORS origins, or *
CORS_ORIGINS=http://localhost:5173
# Optional for P1
GEMINI_API_KEY=

# Ingestion settings (P0)
ARGO_INDEX_URL=https://data-argo.ifremer.fr/ar_index_global_prof.txt
INGEST_DAYS_BACK=7
INGEST_REGION=Indian
INGEST_LIMIT=300
